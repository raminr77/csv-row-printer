{
  "name": "csv-row-printer",
  "license": "ISC",
  "version": "1.2.0",
  "main": "./src/index.js",
  "author": "Ramin Rezaei - ramin.rezaei77@gmail.com",
  "description": "Create & Print Card from CSV Row Data",
  "scripts": {
    "husky": "husky install",
    "format": "npx prettier src/**/*.js --write",
    "sass": "sass --watch src/assets/styles/main.scss:src/assets/styles/main.min.css --style compressed --update"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,scss}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raminr77/csv-row-printer.git"
  },
  "keywords": [
    "CSV",
    "CSV",
    "row",
    "printer"
  ],
  "bugs": {
    "url": "https://github.com/raminr77/csv-row-printer/issues"
  },
  "homepage": "https://github.com/raminr77/csv-row-printer#readme",
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "3.8.1",
    "sass": "^1.97.3"
  },
  "packageManager": "pnpm@10.30.3"
}
