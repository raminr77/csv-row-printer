{
  "name": "csv-row-printer",
  "version": "1.0.0",
  "description": "Create & Print Card from CSV Row Data",
  "main": "./src/index.js",
  "scripts": {
    "husky": "husky install",
    "pretty": "npx prettier src/**/*.js --write",
    "sass": "sass --watch src/assets/styles/main.scss:src/assets/styles/main.min.css --style compressed --update"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,scss}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raminr77/csv-row-printer.git"
  },
  "keywords": [
    "CSV",
    "CSV",
    "row",
    "printer"
  ],
  "author": "Ramin Rezaei",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/raminr77/csv-row-printer/issues"
  },
  "homepage": "https://github.com/raminr77/csv-row-printer#readme",
  "devDependencies": {
    "husky": "^7.0.1",
    "prettier": "2.3.2",
    "sass": "^1.36.0"
  }
}
